<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#256A87"
    tools:context=".ui.configuracion.ConfiguracionActivity">

    <Button
        android:id="@+id/idDatosVehiculo"
        android:layout_width="349dp"
        android:layout_height="64dp"
        android:layout_marginStart="4dp"
        android:backgroundTint="#42AFDD"
        android:text="Datos del VehÃ­culo"
        android:onClick="cambiarADatosVehiculo"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/idPreferencias"
        app:layout_constraintEnd_toEndOf="parent"
        />


    <Button
        android:id="@+id/idPreferencias"
        android:layout_width="349dp"
        android:layout_height="64dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#42AFDD"
        android:onClick="cambiarAPreferencias"
        android:text="Preferencias"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@id/idDatosVehiculo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>